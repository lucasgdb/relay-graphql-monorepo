{
  "name": "@example/server",
  "description": "GraphQL Server template",
  "private": true,
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "start": "knex migrate:latest && node build/server.js",
    "start:dev": "yarn migrate && env-cmd tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "babel src -d build --extensions \".ts\"",
    "build:webpack": "webpack",
    "migrate": "env-cmd knex migrate:latest --knexfile=knexfile.ts",
    "migrate:make": "env-cmd knex migrate:make",
    "seed:make": "env-cmd knex seed:make",
    "seed": "env-cmd knex seed:run",
    "rollback": "env-cmd knex migrate:down",
    "rollback:all": "env-cmd knex migrate:rollback --all"
  },
  "dependencies": {
    "@example/shared": "*",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "core-js": "^3.21.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "glob": "^7.2.0",
    "graphql": "^16.3.0",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-relay": "^0.10.0",
    "jwt-simple": "^0.5.6",
    "knex": "^1.0.3",
    "lodash": "^4.17.21",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/core-js": "^2.5.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.17",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "env-cmd": "^10.1.0",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^2.0.0-0",
    "tsconfig-paths": "^3.12.0"
  }
}
